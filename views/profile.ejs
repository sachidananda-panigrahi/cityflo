<% include header %>
<% include after_login_nav %>
<!--Start inner-header-->
<div class="inner-header">

    <div class="header-content">
        <div class="header-content-inner">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12 text-center">
                        <h1>Account Info</h1>
                        <hr class="primary">
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!--End inner-header-->

<section class="mainbody">
    <div class="container">
        <div class="row">
            <div class="col-lg-6 col-sm-12">

                <h1 class="form-heading">My Profile</h1>

                <div class="booking-warp">
                    <form class="booking">


                        <div class="row">
                            <div class="col-lg-6 col-sm-12">
                                <div class="form-group">
                                    <label>Name</label>
                                    <input type="text" class="form-control" value=" Sopan XYZ " name="user-name">
                                </div>
                            </div><!-- col-sm-6 -->
                            <div class="col-lg-6 col-sm-12">
                                <div class="form-group">
                                    <label>Email</label>
                                    <input type="text" class="form-control" value="xyz8687@gmail.com" name="email">
                                </div>
                            </div><!-- col-sm-6 -->
                        </div><!--End row-->


                        <div class="row">
                            <div class="col-lg-6 col-sm-12">
                                <div class="form-group">
                                    <label>Phone No</label>
                                    <input type="text" class="form-control" value="9998881122 " name="phoneno">
                                </div>
                            </div><!-- col-sm-6 -->
                            <div class="col-lg-6 col-sm-12">
                                <div class="form-group">
                                    <label>Password</label>
                                    <input type="Password" class="form-control" value="XXXXXXXXXX" name="Password">
                                </div>
                            </div><!-- col-sm-6 -->
                        </div><!--End row-->




                        <div class="row">
                            <div class="col-lg-6 col-sm-12">
                                <div class="form-group text-center">
                                    <a class="btn btn-primary btn-xl page-scroll" href="#">Change Password</a>
                                </div>
                            </div><!-- col-sm-6 -->

                            <div class="col-lg-6 col-sm-12">
                                <div class="form-group text-center">
                                    <a class="btn btn-primary btn-xl page-scroll" href="#">Save</a>
                                </div>
                            </div><!-- col-sm-6 -->

                        </div><!--End row-->


                    </form>
                </div>
            </div>
            <!--// End booking details-->




            <div class="col-lg-6 col-sm-12">

                <h1 class="form-heading">My Subscription</h1>

                <div class="booking-warp">
                    <form class="booking">
                        <label>Routes</label>
                        <div class="row">
                            <div class="col-lg-6 col-sm-12">
                                <input type="text" class="form-control" value="Mira Road, Mumbai" name="name">
                            </div>
                            <div class="to"> <span>TO</span> </div>
                            <div class="col-lg-6 col-sm-12">
                                <input type="email" class="form-control" value="BKC, Mumbai" name="email">
                            </div>
                        </div>  <!--End row-->

                        <br>

                        <div class="row">
                            <div class="col-lg-6 col-sm-12">
                                <div class="form-group">
                                    <label>Pick up Point</label>
                                    <input type="text" class="form-control" value="Stop Mira Road, Mumbai" name="pickup-point">
                                </div>
                            </div><!-- col-sm-6 -->
                            <div class="col-lg-6 col-sm-12">
                                <div class="form-group">
                                    <label>Drop Point</label>
                                    <input type="text" class="form-control" value="Stop BKC, Mumbai" name="drop-point">
                                </div>
                            </div><!-- col-sm-6 -->
                        </div><!--End row-->


                        <div class="row">
                            <div class="col-lg-6 col-sm-12">
                                <div class="form-group">
                                    <label>Duration In Months</label>
                                    <input type="text" class="form-control" value="3 Months" name="months">
                                </div>
                            </div><!-- col-sm-6 -->
                            <div class="col-lg-6 col-sm-12">
                                <div class="form-group">
                                    <label>Transaction Amount</label>
                                    <input type="text" class="form-control" value="Rs.3000" name="totalrs">
                                </div>
                            </div><!-- col-sm-6 -->
                        </div><!--End row-->

                        <div class="row">
                            <div class="col-lg-6 col-sm-12">
                                <div class="form-group">
                                    <label>Start Date </label>
                                    <input type="text" class="form-control" value="06/11/2015" name="start-date">
                                </div>
                            </div><!-- col-sm-6 -->
                            <div class="col-lg-6 col-sm-12">
                                <div class="form-group">
                                    <label>End Date</label>
                                    <input type="text" class="form-control" value="09/11/2015" name="end-date">
                                </div>
                            </div><!-- col-sm-6 -->
                        </div><!--End row-->


                        <div class="row">
                            <div class="col-lg-12 col-sm-12">
                                <div class="form-group text-center">
                                    <a class="btn btn-primary btn-xl page-scroll" href="#">Renew My Plan</a>
                                </div>
                            </div><!-- col-sm-12 -->

                        </div><!--End row-->


                    </form>
                </div>
            </div>
            <!--// End Payment details-->


        </div>
    </div>
</section>

